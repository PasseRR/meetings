<!-- type默认weekly 跟job的group对应 -->
<jobs>
    <weekly>
        <name>技术双周例会</name> <!-- 会议名字 -->
        <roomName>会议室1</roomName> <!-- 会议室名字 -->
        <publisher>虞立昂</publisher> <!-- 会议发布人 -->
        <startTime>15:00:00</startTime> <!-- 会议开始时间 格式HH:mm:ss -->
        <endTime>17:00:00</endTime> <!-- 会议结束时间 格式HH:mm:ss -->
        <effectiveDate>2017-01-08</effectiveDate> <!-- 开始执行任务日期 必须任务开始周的周日 -->
        <interval>2</interval>
        <dayOfWeek>4</dayOfWeek> <!-- 星期几 -->
    </weekly>
</jobs>